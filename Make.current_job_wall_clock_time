#! /bin/ksh
# generer r.current_job_wall_clock_time
RMNLIB_REVISION=$1
if [ ${BASE_ARCH} = AIX-powerpc7 ] ; then
  PARAMS="-librmn ${RMNLIB_REVISION} -libsys llapi"
else
  PARAMS=""
fi
r.compile -o r.current_job_wall_clock_time_0.0-${BASE_ARCH} ${PARAMS} -src r.current_job_wall_clock_time.ftn
exit $?
